= form_for @park do |f|
  - if @park.errors.any?
    #error_explanation
      %h2= "#{pluralize(@park.errors.count, "error")} prohibited this park from being saved:"
      %ul
        - @park.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :url
    = f.text_field :url
  .field
    = f.label :name
    = f.text_field :name
  .actions
    = f.submit 'Save'
