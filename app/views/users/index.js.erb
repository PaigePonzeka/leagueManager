var $usersContainer = $(".js-available-users");
var $usersResultsContainer = $("#available-users-results");
resetSearch();

<% if @users.any? %>
  $usersResultsContainer.append("<%=@users.count %> User(s) Found" );
  <% @users.each do |user| %>
    $usersContainer.append("<option value=<%=user.id%>><%=name(user) %> <%=user.email %><li>");
  <% end %>
<%else%>
  $usersResultsContainer.html("No Users Found"); 
<% end %>

function resetSearch(){
  $usersContainer.html("");
  $usersResultsContainer.html("");
}